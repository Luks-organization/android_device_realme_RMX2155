binder_call(system_app, hal_gatekeeper_default)
binder_call(system_app, mtk_hal_bluetooth)
binder_call(system_app, hal_wifi_default)
binder_call(system_app, mtk_hal_bluetooth)
binder_call(system_app, mtk_hal_memtrack)
binder_call(system_app, mtk_hal_usb)
binder_call(system_app, wificond)
binder_call(system_app, netd)
binder_call(system_app, vold)
