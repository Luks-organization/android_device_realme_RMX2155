net_domain(system_app)
binder_use(system_app)

allow system_app sysfs_zram:dir search;
allow system_app sysfs_zram:file r_file_perms;
allow system_app sysfs_batteryinfo:dir { search };
allow system_app sysfs_batteryinfo:file rw_file_perms;
allow system_app proc_pagetypeinfo:file r_file_perms;
allow system_app proc_asound:dir { search };
allow system_app proc_asound:file r_file_perms;
allow system_app zygote:unix_stream_socket { getopt };
allow system_app vendor_sysfs_graphics:dir { search };
allow system_app vendor_sysfs_graphics:file rw_file_perms;
allow system_app vendor_sysfs_usb_supply:dir { search };
allow system_app vendor_sysfs_usb_supply:file rw_file_perms;
allow system_app vendor_sysfs_otg_switch:dir { search };
allow system_app vendor_sysfs_otg_switch:file rw_file_perms;
allow system_app vendor_sysfs_battery_supply:dir { search };
allow system_app vendor_sysfs_battery_supply:file rw_file_perms;
allow system_app vendor_proc_display:dir { search };
allow system_app vendor_proc_display:file rw_file_perms;
allow system_app vendor_proc_batt_param:file { getattr read };
allow system_app vendor_proc_charger:dir { search read };
allow system_app vendor_proc_charger:file r_file_perms;
allow system_app sysfs_therm:file r_file_perms;
allow system_app mtk_hal_bluetooth:service_manager find;
allow system_app mtk_hal_bluetooth:binder { transfer call };
allow system_app netd:service_manager find;
allow system_app netd:binder { transfer call };
allow system_app proc_filesystems:file r_file_perms;

get_prop(system_app, vendor_variant_prop)
