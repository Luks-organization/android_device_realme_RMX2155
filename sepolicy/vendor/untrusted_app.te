allow untrusted_app vendor_sysfs_battery_supply:dir { search };
allow untrusted_app vendor_sysfs_battery_supply:file { read };
allow untrusted_app vendor_proc_batt_param:file { getattr };
allow untrusted_app vendor_proc_charger:dir { search };
allow untrusted_app vendor_proc_charger:file { read };
allow untrusted_app vendor_proc_display:dir { search };
allow untrusted_app vendor_proc_display:file { read };
allow untrusted_app vendor_proc_nfc_chipset:dir { search };
allow untrusted_app vendor_proc_nfc_chipset:file { getattr };
allow untrusted_app vendor_proc_oplus_sensor_feature:dir { search };
allow untrusted_app vendor_proc_oplus_sensor_feature:file { read };
allow untrusted_app vendor_proc_oplus_version:dir { search };
allow untrusted_app zygote:unix_stream_socket { getopt };
