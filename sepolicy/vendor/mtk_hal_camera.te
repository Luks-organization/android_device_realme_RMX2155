r_dir_file(mtk_hal_camera, vendor_proc_oplus_version)

get_prop(mtk_hal_camera, vendor_mtk_camera_prop)
get_prop(mtk_hal_camera, system_oplus_project_prop)
get_prop(mtk_hal_camera, default_prop)

set_prop(mtk_hal_camera, system_oplus_project_prop)
set_prop(mtk_hal_camera, vendor_mtk_camera_prop)

binder_call(mtk_hal_camera, mtk_hal_mmagent)
binder_call(mtk_hal_camera, cameraserver)
