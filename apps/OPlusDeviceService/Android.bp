android_app {
    name: "DeviceServiceOplus",

    srcs: ["src/**/*.java"],
    resource_dirs: ["res"],

    certificate: "platform",
    platform_apis: true,
    optimize: {
        enabled: false,
    },
    required: [
        "privapp-permissions-oplus-device",
    ],

    system_ext_specific: true,
    privileged: true,
}

prebuilt_etc {
    name: "privapp-permissions-oplus-device",
    sub_dir: "permissions",
    src: "privapp-permissions-oplus-device.xml",
    filename_from_src: true,
    system_ext_specific: true,
}
